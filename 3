#include <iostream>
using namespace std;
int main()
{
    int n, i, min;
    cin>>n;
    int a[n];
    min=10000000;
    for (i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for (i=0; i<n; i++)
    {
        if (i%2==0 && a[i]<min)
        {
            min=a[i];
        }
    }
    cout<<min;
    return 0;
}
